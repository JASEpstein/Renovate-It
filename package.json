{
  "name": "mern",
  "version": "1.0.0",
  "description": "Mern Demo",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "cd client && npm run start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "cd client npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.19.1"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bcryptjs": "2.4.3",
    "body-parser": "1.19.0",
    "concurrently": "4.1.0",
    "dotenv": "8.0.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "is-empty": "1.2.0",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.11",
    "mongoose": "~5.6.0",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "validator": "11.0.0"
  }
}
